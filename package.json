  },
  "homepage": "https://github.com/a-k-kord/mf.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
