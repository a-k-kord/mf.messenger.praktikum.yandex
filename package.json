{
  "name": "mf.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "Приложение Чат.",
  "main": "index.js",
  "engine": {
    "node": "~15.2.1"
  },
  "scripts": {
    "compile:scss": "node-sass static/scss/main.scss static/css/style.css",
    "watch:scss": "node-sass static/scss/*.scss static/css/style.css -w",
    "copy:assets": "cp -r static/* dist",
    "build": "./node_modules/.bin/tsc --project ./tsconfig.json --watch",
    "check-types": "tsc",
    "compile": "babel src -d static/js --extensions '.spec.ts'",
    "prepublish": "npm run check-types && npm run compile",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha"
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-k-kord/mf.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "Andrey Korotayev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-k-kord/mf.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/a-k-kord/mf.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.31",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.3.0",
    "express": "^4.17.1",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^8.3.0",
    "node-sass": "^5.0.0",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "eta": "^1.12.1"
  }
}
